                    <script>
                        // Chart.js Arabic RTL support
                        Chart.defaults.font.family = 'Cairo, sans-serif';

                        // Status labels mapping
                        const statusLabels = {
                            received: 'تم الاستلام',
                            under_review: 'قيد المراجعة',
                            approved_payment_pending: 'بانتظار الدفع',
                            approved_payment_required: 'مطلوب الدفع',
                            payment_verified: 'تم التحقق',
                            completed: 'مكتمل',
                            rejected: 'مرفوض',
                            ready: 'جاهز'
                        };

                        // Type labels mapping
                        const typeLabels = {
                            fisherman: 'ترخيص صياد',
                            boat: 'ترخيص مركب',
                            vehicle: 'ترخيص سيارة',
                            trade: 'ترخيص تجارة',
                            entry: 'ترخيص دخول',
                            other: 'أخرى'
                        };

                        // Status colors (object mapping for accurate color matching)
                        const statusColors = {
                            received: '#2196f3',
                            under_review: '#ff9800',
                            approved_payment_pending: '#ffc107',
                            approved_payment_required: '#ff5722',
                            payment_verified: '#00bcd4',
                            completed: '#4caf50',
                            rejected: '#f44336',
                            ready: '#8bc34a'
                        };

                        // Type colors
                        const typeColors = ['#2196f3', '#00897b', '#ff9800', '#9c27b0'];

                        // Parse data from server
                        const byStatusData = <%- JSON.stringify(stats.byStatus || []) %>;
                        const byTypeData = <%- JSON.stringify(stats.byType || []) %>;
                        const monthlyTrendData = <%- JSON.stringify(stats.monthlyTrend || []) %>;
                        const revenueData = <%- JSON.stringify(monthlyRevenue || []) %>;

                        console.log('=== DEBUG DATA ===');
                        console.log('byStatusData:', byStatusData);
                        console.log('byTypeData:', byTypeData);
                        console.log('monthlyTrendData:', monthlyTrendData);
                        console.log('revenueData:', revenueData);

                        // Status Pie Chart
                        const statusCtx = document.getElementById('statusChart').getContext('2d');
                        console.log('Status Chart Data:', byStatusData);
                        if (byStatusData && byStatusData.length > 0) {
                            new Chart(statusCtx, {
                                type: 'pie',
                                data: {
                                    labels: byStatusData.map(item => statusLabels[item.status] || item.status),
                                    datasets: [{
                                        data: byStatusData.map(item => parseInt(item.count)),
                                        backgroundColor: byStatusData.map(item => statusColors[item.status] || '#9e9e9e'),
                                        borderWidth: 2,
                                        borderColor: '#fff'
                                    }]
                                },
                                options: {
                                    responsive: true,
                                    maintainAspectRatio: false,
                                    plugins: {
                                        legend: {
                                            position: 'bottom',
                                            rtl: true,
                                            labels: {
                                                usePointStyle: true,
                                                padding: 15
                                            }
                                        },
                                        tooltip: {
                                            callbacks: {
                                                label: function (context) {
                                                    const total = context.dataset.data.reduce((a, b) => a + b, 0);
                                                    const percentage = ((context.raw / total) * 100).toFixed(1);
                                                    return `${context.label}: ${context.raw} (${percentage}%)`;
                                                }
                                            }
                                        }
                                    }
                                }
                            });
                        } else {
                            console.log('No status data available for chart');
                        }

                        // Type Doughnut Chart
                        const typeCtx = document.getElementById('typeChart').getContext('2d');
                        new Chart(typeCtx, {
                            type: 'doughnut',
                            data: {
                                labels: byTypeData.map(item => typeLabels[item.applicationType] || item.applicationType),
                                datasets: [{
                                    data: byTypeData.map(item => parseInt(item.count)),
                                    backgroundColor: typeColors,
                                    borderWidth: 2,
                                    borderColor: '#fff'
                                }]
                            },
                            options: {
                                responsive: true,
                                maintainAspectRatio: false,
                                plugins: {
                                    legend: {
                                        position: 'bottom',
                                        rtl: true,
                                        labels: {
                                            usePointStyle: true,
                                            padding: 15
                                        }
                                    }
                                }
                            }
                        });

                        // Monthly Trend Line Chart
                        const trendCtx = document.getElementById('trendChart').getContext('2d');
                        const sortedTrend = monthlyTrendData.sort((a, b) => new Date(a.date) - new Date(b.date));
                        new Chart(trendCtx, {
                            type: 'line',
                            data: {
                                labels: sortedTrend.map(item => new Date(item.date).toLocaleDateString('ar-EG', { month: 'short', day: 'numeric' })),
                                datasets: [{
                                    label: 'الطلبات',
                                    data: sortedTrend.map(item => parseInt(item.count)),
                                    borderColor: '#1e3c72',
                                    backgroundColor: 'rgba(30, 60, 114, 0.1)',
                                    fill: true,
                                    tension: 0.4,
                                    pointBackgroundColor: '#1e3c72',
                                    pointBorderColor: '#fff',
                                    pointBorderWidth: 2,
                                    pointRadius: 5
                                }]
                            },
                            options: {
                                responsive: true,
                                maintainAspectRatio: false,
                                plugins: {
                                    legend: {
                                        display: false
                                    }
                                },
                                scales: {
                                    y: {
                                        beginAtZero: true,
                                        ticks: {
                                            stepSize: 1
                                        }
                                    }
                                }
                            }
                        });

                        // Revenue Bar Chart
                        const revenueCtx = document.getElementById('revenueChart').getContext('2d');
                        new Chart(revenueCtx, {
                            type: 'bar',
                            data: {
                                labels: revenueData.map(item => item.month),
                                datasets: [{
                                    label: 'الإيرادات (جنيه)',
                                    data: revenueData.map(item => parseFloat(item.amount) || 0),
                                    backgroundColor: 'rgba(0, 137, 123, 0.8)',
                                    borderColor: '#00897b',
                                    borderWidth: 1,
                                    borderRadius: 8
                                }]
                            },
                            options: {
                                responsive: true,
                                maintainAspectRatio: false,
                                plugins: {
                                    legend: {
                                        display: false
                                    }
                                },
                                scales: {
                                    y: {
                                        beginAtZero: true,
                                        ticks: {
                                            callback: function (value) {
                                                return value.toLocaleString('ar-EG') + ' ج.م';
                                            }
                                        }
                                    }
                                }
                            }
                        });

                        function refreshData() {
                            window.location.reload();
                        }
                    </script>
</body>

</html>